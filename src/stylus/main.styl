.bs {
  height: 2000px
}

.past {
  fill: #283048
  }

.delay {
  fill: #283048
}

.future {
  fill: #859398
}

.chart text {
  fill: white;
  font: 10px sans-serif;
  text-anchor: middle;
}

.axis text {
  font: 10px sans-serif;
